{
  "name": "npm-angular2",
  "version": "1.0.0",
  "description": "Getting started with Angular 2 development using NPM",
  "scripts": {
    "postinstall": "npm run typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "typings": "typings"
  },
  "keywords": [
    "angular2",
    "npm"
  ],
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.22",
    "three": "^0.74.0",
    "zone.js": "0.5.15"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.2",
    "typings": "^0.6.8"
  },
  "author": "Brian Sidebotham <brian.sidebotham@gmail.com>",
  "license": "MIT",
  "repository": "git+https://github.com/BrianSidebotham/npm-angular2"
}
